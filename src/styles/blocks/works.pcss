.works-section {
  position: relative;
  background-color: $white;
  overflow: visible;
  z-index: 1;
}

.works-section__container {
  min-height: 650px;
  padding: 150px 20px;
}

.works-section__title {
  font-size: 36px;
  font-weight: bold;
  color: #333c49;
  margin-bottom: 60px;
}

.works-section__content {
  display: flex;
  justify-content: center;
  width: 100%;
  height: 100%;
}
